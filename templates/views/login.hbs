<!DOCTYPE html>
<html>
  <head>
    <title>Login</title>
    <meta http-equiv="X-UA-Compatible" content="IE=edge"/>
    <meta http-equiv="content-type" content="text/html; charset=utf-8"/>

 
    <link rel="stylesheet" href="/css/login/login.css">
    <script src="/js/jquery-3.2.1.min.js"></script>

    <script src="/js/retailown.js"></script>


  </head>
  
  <body>
 
    <div id="base" class="">

      <!-- Login Button (Rectangle) -->
      <div id="u0" class="ax_default primary_button" data-label="Login Button" >
        <div id="u0_div" class=""></div>
        <div id="u0_text" class="text" onclick="Start()">
          <p id="login"><span>Login</span></p>
        </div>
      </div>

      <!-- +DLogin (Dynamic Panel) -->
      <div id="u1" class="ax_default" data-label="+DLogin">
        <div id="u1_state0" class="panel_state" data-label="Login" style="">
          <div id="u1_state0_content" class="panel_state_content">

            <!-- Unnamed (Rectangle) -->
            <div id="u2" class="ax_default heading_1">
              <div id="u2_div" class=""></div>
              <div id="u2_text" class="text ">
                <p id="pp"><span>Welcome to TCS Video Analytics Solutions in Azure </span></p>
              </div>
            </div>

            <!-- Unnamed (Image) -->
            <div id="u3" class="ax_default image">
              <img id="u3_img" class="img " src="/img/u3.png"/>
              <div id="u3_text" class="text " style="display:none; visibility: hidden">
                <p></p>
              </div>
            </div>

            <!-- Unnamed (Rectangle) -->
            <div id="u4" class="ax_default heading_2">
              <div id="u4_div" class=""></div>
              <div id="u4_text" class="text ">
                <p><span>Email / Username</span></p>
              </div>
            </div>

            <!-- Unnamed (Rectangle) -->
            <div id="u5" class="ax_default heading_2">
              <div id="u5_div" class=""></div>
              <div id="u5_text" class="text ">
                <p><span>Password</span></p>
              </div>
            </div>

            <!-- Login Password (Text Field) -->
            <div id="u6" class="ax_default text_field" data-label="Login Password">
              <div id="u6_div" class=""></div>
              <input id="u6_input" type="password" value="" class="u6_input"/>
            </div>

            <!-- +Login (Rectangle) -->
            <div id="u7" class="ax_default primary_button" data-label="+Login" onclick="login()">
              <div id="u7_div" class=""></div>
              <div id="u7_text" class="text ">
                <p><span>Login</span></p>
              </div>
            </div>

            <!-- Unnamed (Rectangle) -->
            <div id="u8" class="ax_default button" onclick="sign()">
              <div id="u8_div" class=""></div>
              <div id="u8_text" class="text ">
                <p><span>Signup</span></p>
              </div>
            </div>

            <!-- Unnamed (Shape) -->
            <div id="u9" class="ax_default primary_button">
              <img id="u9_img" class="img " src="/img/u9.svg"/>
              <div id="u9_text" class="text ">
                <p><span>Forgot Password</span></p>
              </div>
            </div>

            <!-- Unnamed (Text Field) -->
            <div id="u10" class="ax_default text_field">
              <div id="u10_div" class=""></div>
              <input id="u10_input" type="email" value="" class="u10_input"/>
            </div>

            <!-- +ErrorPannel (Dynamic Panel) -->
            <div id="u11" class="ax_default ax_default_hidden" data-label="+ErrorPannel" style="display:none; visibility: hidden">
              <div id="u11_state0" class="panel_state" data-label="Error" style="">
                <div id="u11_state0_content" class="panel_state_content">

                  <!-- Unnamed (Rectangle) -->
                  <div id="u12" class="ax_default flow_shape">
                    <div id="u12_div" class=""></div>
                    <div id="u12_text" class="text ">
                      <p><span>Check the credentials and try again</span></p>
                    </div>
                  </div>

                  <!-- Unnamed (Shape) -->
                  <div id="u13" class="ax_default icon">
                    <img id="u13_img" class="img " src="/img/u13.svg"/>
                    <div id="u13_text" class="text " style="display:none; visibility: hidden">
                      <p></p>
                    </div>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
    </div>
    <!-- <script src="resources/scripts/axure/ios.js"></script> -->
  </body>
</html>
